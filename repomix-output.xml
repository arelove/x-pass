                  {/* Status Chips */}
                  <Box display="flex" gap={1} alignItems="center" mb={1.5} flexWrap="wrap">
                    <Chip
                      label={
                        pseudoModeEnabled
                          ? t('settings.pseudoMode.enabled')
                          : t('settings.pseudoMode.disabled')
