                  <IconButton
                    sx={{
                      position: 'absolute',
                      bottom: 0,
                      right: 0,
                      bgcolor: 'primary.main',
                      color: 'white',
                      '&:hover': { bgcolor: 'primary.dark' },
                    }}
                    size="small"
                    onClick={() => profilePicInputRef.current?.click()}
                  >
                    <CameraAltIcon fontSize="small" />
