import i18n from 'i18next';
import { initReactI18next } from 'react-i18next';

// Импортируем все переводы (предполагаем, что файлы уже есть в public/locales)
import translationEN from './assets/locales/en/translation.json';
import translationRU from './assets/locales/ru/translation.json';
import translationZH from './assets/locales/zh/translation.json'; // упрощённый китайский
import translationES from './assets/locales/es/translation.json'; // испанский
import translationPT from './assets/locales/pt/translation.json'; // португальский
import translationFR from './assets/locales/fr/translation.json'; // французский
import translationDE from './assets/locales/de/translation.json'; // немецкий
import translationJA from './assets/locales/ja/translation.json'; // японский
import translationKO from './assets/locales/ko/translation.json'; // корейский
import translationIT from './assets/locales/it/translation.json'; // итальянский
import translationAR from './assets/locales/ar/translation.json'; // арабский
import translationHI from './assets/locales/hi/translation.json'; // хинди
import translationID from './assets/locales/id/translation.json'; // индонезийский

const resources = {
  en: { translation: translationEN },
  ru: { translation: translationRU },
  zh: { translation: translationZH }, // Китайский (упрощённый)
  es: { translation: translationES }, // Испанский
  pt: { translation: translationPT }, // Португальский (Бразилия/Португалия)
  fr: { translation: translationFR }, // Французский
  de: { translation: translationDE }, // Немецкий
  ja: { translation: translationJA }, // Японский
  ko: { translation: translationKO }, // Корейский
  it: { translation: translationIT }, // Итальянский
  ar: { translation: translationAR }, // Арабский
  hi: { translation: translationHI }, // Хинди
  id: { translation: translationID }, // Индонезийский
};

i18n
  .use(initReactI18next)
  .init({
    resources,
    lng: localStorage.getItem('language') || 'en',
    fallbackLng: 'en',
    interpolation: {
      escapeValue: false,
    },
    // Добавляем поддержку RTL для арабского
    react: {
      useSuspense: false,
    },
  });

// Для поддержки RTL (арабский)
i18n.on('languageChanged', (lng) => {
  document.documentElement.dir = lng === 'ar' ? 'rtl' : 'ltr';
});

export default i18n;
